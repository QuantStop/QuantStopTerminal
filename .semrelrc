{
  "maintainedVersion": "prerelease",
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0"
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "provider": {
      "name": "github",
      "options": {
        "github_use_compare_commits": "true"
      }
    },
    "files-updater": {
      "names": ["npm"]
    }
  }
}
