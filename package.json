{
  "name": "@quantstop/quantstopterminal",
  "version": "0.2.9",
  "description": "Trading & Analysis Platform",
  "author": "quantstop",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantstop/quantstopterminal.git"
  },
  "scripts": {
    "dev": "vite --config ./web/vite.config.ts",
    "build": "run-p type-check build-only",
    "preview": "vite --config web/vite.config.ts preview --port 4173",
    "build-only": "vite build --config ./web/vite.config.ts ",
    "type-check": "vue-tsc -p ./web/tsconfig.json --noEmit",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "release",
      "next",
      "next-major",
      "+([0-9])?(.{+([0-9]),x}).x",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/changelog",
      {
        "changelogFile": "./docs/CHANGELOG.md"
      },
      "@semantic-release/npm",
      {
        "npmPublish": false
      },
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "./docs/CHANGELOG.md",
            "./package.json",
            "package-lock.json"
          ],
          "message": "chore(release): set `version` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^19.0.5",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  }
}
